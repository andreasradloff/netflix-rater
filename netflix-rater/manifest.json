{
	"manifest_version": 2,

	"name": "Netflix Rater for Filmtipset",
	"description": "Bulk sets some Netflix ratings from a JSON input",
	"version": "1.0",

	"browser_action": {
		"default_title": "Rate!"
	},

	"permissions": [
		"http://*.netflix.com/",
		"https://www.googleapis.com/freebase/v1/mqlread/"
	],

	"background": {
		"scripts": ["background.js"]
	}
}
